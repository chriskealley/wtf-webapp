{
  "name": "WTF Vue web app template",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "A Vue webapp template for the WFT collection",
  "author": "Chris Kealley",
  "repository": {
    "type": "git",
    "url": "https://github.com/chriskealley/wtf-webapp"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "prepare": "husky install",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-router": "4"
  },
  "devDependencies": {
    "@iconify-json/ph": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@unocss/eslint-config": "^0.51.6",
    "@unocss/preset-attributify": "^0.51.6",
    "@unocss/preset-icons": "^0.51.6",
    "@unocss/reset": "^0.51.6",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "unocss": "^0.51.6",
    "vite": "^4.3.0",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,vue}": "eslint --fix",
    "**/*": "prettier --write --ignore-unknown"
  }
}
